{
    "rules": {
        "react/jsx-indent": ["error", 4],
        "react/jsx-indent-props": ["error", 4],
        "no-unused-expressions": ["error", {
            "allowTaggedTemplates": true
        }],
        "react/jsx-no-bind": "off"
    },
    "env": {
        "browser": true,
        "es6": true
    }
}